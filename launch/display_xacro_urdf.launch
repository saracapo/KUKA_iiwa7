<?xml version="1.0" ?>

<launch>
	<arg name="model" />
	<param name="robot_description" command="$(find xacro)/xacro $(find KUKA_iiwa_pkg)/urdf/KUKA_iiwa.xacro" />
	<param name="use_gui" value="true"/>

	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
	
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find KUKA_iiwa_pkg)/urdf.rviz" required="true" />

</launch>
